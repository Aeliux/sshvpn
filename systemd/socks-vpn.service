[Unit]
Description=Socks5-based VPN Service
After=network-online.target socks-vpn-server.service
Wants=socks-vpn-server.service

[Service]
Type=simple
ExecStart=/usr/local/bin/socks-vpn-control daemon
ExecStop=/usr/local/bin/socks-vpn-control stop
Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target
