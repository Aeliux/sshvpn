[Unit]
Description=SSH dynamic socks
After=network-online.target
PartOf=socks-vpn.service

[Service]
Type=simple
ExecStart=/usr/bin/sshpass -f /home/aeliux/.ssh/vpn.pass /usr/bin/ssh -F /home/aeliux/.ssh/config -o ExitOnForwardFailure=yes -o ServerAliveInterval=60 -N -D 127.0.0.1:1080 vpn-dev
Restart=on-failure
RestartSec=5

[Install]
WantedBy=default.target
